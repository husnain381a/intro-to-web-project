        </div>
    </nav>
    <!-- Navbar Ends -->
    <!-- Main Banner Starts -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/assets/contact-us.jpg" class="d-block w-100" id="ai_img" alt="..." />
        </div>
    </div>
    <!-- Main Banner Ends -->
    <!-- Contact Us Form + Text Starts -->
    <div class="c_container">
        <div class="c_main">
            <div class="c_items">
                <div class="c_item1">
                    <img src="~/assets/address-pin-location-svgrepo-com.svg" width="40px" alt="">
                    <div>
                        <h3>Address</h3>
                        <p>Punjab, Pakistan</p>
                    </div>
                </div>
                <div class="c_item2">
                    <img src="~/assets/call-receive-svgrepo-com.svg" width="40px" alt="">
                    <div>
                        <h3>Call Us</h3>
                        <p>+92 324-540-3464</p>
                    </div>
                </div>
                <div class="c_item3">
                    <img src="~/assets/email-8-svgrepo-com.svg" width="40px" alt="">
                    <div>
                        <h3>Email Us</h3>
                        <p>info@openskillssource.com</p>
                    </div>
                </div>
            </div>

            

                <form method="post" action="/Contact/SubmitForm">
                    <div>
                        <label for="Name">Name:</label>
                        <input type="text" id="Name" name="Name" required />
                    </div>
                    <div>
                        <label for="Email">Email:</label>
                        <input type="email" id="Email" name="Email" required />
                    </div>
                    <div>
                        <label for="Message">Message:</label>
                        <textarea id="Message" name="Message" required></textarea>
                    </div>
                    <div>
                        <button type="submit">Submit</button>
                    </div>
                </form>
                @if (ViewBag.SubmittedData != null)
                {
                    <div class="submitted-data">
                        <h3>Your Submitted Data:</h3>
                        <p><strong>Name:</strong> @ViewBag.SubmittedData.Name</p>
                        <p><strong>Email:</strong> @ViewBag.SubmittedData.Email</p>
                        <p><strong>Message:</strong> @ViewBag.SubmittedData.Message</p>
                    </div>
                }

            
        </div>
    </div>

